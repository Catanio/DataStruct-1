#include <stdlib.h>
#include <stdio.h>

void quadrado (int *a, int *b)
{
	int *A = (int *)malloc(sizeof(int));
	int *B = (int *)malloc(sizeof(int));
	*A=(*a)*(*a);
	*B=(*b)*(*b);
	*b=*B;
	*a=*A;
	free(A);
	free(B);
}

int main (argc, argv)
{
	int a, b;
	printf("informe dois valores: ");
	scanf("%d %d", &a, &b);
	
	quadrado(&a, &b);
	printf("\n os quadrados sao %d e %d\n", a, b);
	
	return 0;
}
